---
layout: null
---
[
{% for link in site.data.links %}
  {% if forloop.index > 1 %},{% endif %}{
    "title": "{{ link.title }}",
    "description": "{{ link.description }}",
    "url": "{{ link.url }}",
    "categories": [
    {% for category in link.categories %}
      {
        "name": "{{category}}",
        "human": "{{ site.data.categories[category] }}"
      }{% unless forloop.last %},{% endunless %}
    {% endfor %}
    ]
  }
{% endfor %}
]
